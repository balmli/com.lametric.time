
{
  "id": "lametric",
  "name": {
    "en": "Lametric Time"
  },
  "$extends": [
    "defaults"
  ],
  "class": "other",
  "capabilities": [
    "volume_set"
  ],
  "discovery": "lametric_time",
  "pair": [
    {
      "id": "login_oauth2",
      "template": "login_oauth2"
    },
    {
      "id": "list_devices",
      "template": "list_devices",
      "navigation": {
        "next": "add_devices"
      }
    },
    {
      "id": "add_devices",
      "template": "add_devices"
    }
  ],
  "repair": [
    {
      "id": "login_oauth2",
      "template": "login_oauth2"
    }
  ]
}
